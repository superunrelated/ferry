{
    "name": "ferry",
    "compatibility_date": "2024-01-01",
    "site": {
        "bucket": "./dist/apps/ferry"
    },
    "build": {
        "install": "npm install --force",
        "command": "npm run build",
        "cwd": "."
    },
    "redirects": [
        {
            "from": "/*",
            "to": "/index.html",
            "status": 200
        }
    ]
}